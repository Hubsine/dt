{% extends "DtAdminBundle::layout.html.twig" %}

{% form_theme form 'MopaBootstrapBundle:form:fields.html.twig' %}

{% block headline %}{% trans %} dashboard.about_users.add {% endtrans %}{% endblock headline %}

{% block content_page_content %}
    
    {{ form_start(form, {'method': 'post', 'action': path('dt_admin_about_users_add'), 'attr': {'class': ''}}) }}

