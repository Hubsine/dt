{% extends "DtAdminBundle::layout.html.twig" %}

{% block headline %}{% trans %} dashboard.about_user_meta.about_user_meta {% endtrans %}{% endblock headline %}

{% block content_page_content %}

    <table class="table table-striped table-responsive">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>Onproperty</th>
                <th>Deletedat</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for lookingForMetum in lookingForMetas %}
            <tr>
                <td><a href="{{ path('dt_admin.looking_for_meta.show', { 'id': lookingForMetum.id }) }}">{{ lookingForMetum.id }}</a></td>
                <td>{{ lookingForMetum.label }}</td>
                <td>{{ lookingForMetum.onProperty }}</td>
                <td>{% if lookingForMetum.deletedAt %}{{ lookingForMetum.deletedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if lookingForMetum.createdAt %}{{ lookingForMetum.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if lookingForMetum.updatedAt %}{{ lookingForMetum.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('dt_admin.looking_for_meta.show', { 'id': lookingForMetum.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('dt_admin.looking_for_meta.edit', { 'id': lookingForMetum.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('dt_admin.looking_for_meta.new') }}">Create a new lookingForMetum</a>
        </li>
    </ul>
{% endblock content_page_content %}
