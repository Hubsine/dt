<div class="col-xs-3 col-md-3 col-lg-3 userPictureItem">

    {% set pictureUrl = 
        [
            constant('USER_UPLOADED_FOLDER', picture)|replace({'%d': app.user.id}),
            picture.name
        ]|join('/') 
    %}

    <div class="thumbnail">
        <img src="{{ picture|getUserPictureUrl }}">
        {% include 'DtUserBundle:Compte:UserPicture/edit.html.twig' %}
    </div>

</div>